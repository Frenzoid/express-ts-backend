{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb", "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "no-console": [false, "log", "error"],
    "eofline": false,
    "indent": false,
    "no-empty-interface": false,
    "no-empty": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "no-unused-expression": false,
    "quotemark": [
      true,
      "single"
    ],
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "max-line-length": [
      true,
      150
    ],
    "member-ordering": [
      false
    ],
    "curly": false,
    "interface-name": [
      false
    ],
    "array-type": [
      false
    ],
    "max-classes-per-file": [
      false, 1
    ],
    "variable-name": [
      false
    ],
    "one-line": [
      false
    ],
    "one-variable-per-declaration": [
      false
    ]
  },
  "rulesDirectory": []
}